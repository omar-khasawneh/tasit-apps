{
  "expo": {
    "name": "Tasit Wallet",
    "slug": "wallet-deep-linking",
    "privacy": "unlisted",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.0.4",
    "orientation": "portrait",
    "icon": "./assets/images/TasitWalletExampleIcon1024.png",
    "scheme": "wallet-deep-linking",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#07FFC2"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.tasit.wallet",
      "buildNumber": "4",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "wallet-deep-linking"
        ]
      }
    },
    "android": {
      "package": "io.tasit.wallet",
      "permissions": [],
      "versionCode": 4
    }
  }
}
