{
  "main": "node_modules/expo/AppEntry.js",
  "version": "0.0.1",
  "scripts": {
    "start": "npx expo start -c",
    "android": "npx expo start --android",
    "ios": "npx expo start --ios",
    "build:android": "npx expo build:android",
    "build:ios": "npx expo build:ios",
    "lint": "npx prettier --write '{*.js,**/*.js,*.jsx,**/*.jsx}' && npx eslint '{*.js,**/*.js,*.jsx,**/*.jsx,**/*.snap}' --fix",
    "eject": "npx expo eject",
    "pretest": "npm run lint",
    "test:update": "npx jest -u --maxWorkers=2",
    "test": "npx jest --maxWorkers=2"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tasitlabs/tasit.git"
  },
  "dependencies": {
    "expo": "^32.0.0",
    "react": "16.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz",
    "react-navigation": "^3.9.2",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-preset-expo": "^5.0.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-native": "^3.7.0",
    "expo-cli": "^2.17.3",
    "jest": "^24.8.0",
    "jest-expo": "^32.0.0",
    "prettier": "^1.17.1"
  },
