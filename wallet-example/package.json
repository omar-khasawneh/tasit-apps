  "devDependencies": {
    "babel-preset-expo": "^5.0.0",
    "eslint": "^5.16.0",
    "expo-cli": "^2.17.3",
    "jest": "^24.8.0",
    "jest-expo": "^32.0.0",
    "prettier": "^1.17.1"
