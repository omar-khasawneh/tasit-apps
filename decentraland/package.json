{
  "name": "tasit-decentraland",
  "version": "0.0.24",
  "description": "An app for buying Decentraland land using the Tasit SDK",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "setup:ropsten": "cp config/ropsten.ts config/current.ts",
    "setup:rinkeby": "cp config/rinkeby.ts config/current.ts",
    "start": "Fnpm run start:bare -- --config development",
    "start:bare": "FORCE_COLOR=1 ts-node starter.ts",
    "start:ropsten": "npm run start:bare -- --config ropsten",
    "start:rinkeby": "npm run start:bare -- --config rinkeby",
    "build:android": "npm run setup:ropsten && npx expo build:android",
    "build:ios": "npm run setup:ropsten && npx expo build:ios",
    "build:dev:android": "npm run build:android -- --release-channel dev",
    "build:dev:ios": "npm run build:ios --release-channel dev",
    "build:stag:android": "npm run build:android --release-channel stag",
    "build:stag:ios": "npm run build:ios --release-channel stag",
    "deploy:safe": "ts-node scripts/gnosisSafe.ts",
    "publish": "npm run setup:ropsten && npx expo publish",
    "eject": "npx expo eject",
    "pretest": "npm run lint",
    "test:update": "npx jest -u --maxWorkers=2",
    "test": "npx jest --maxWorkers=2",
    "lint": "npx prettier --write '{*.js,**/*.js,*.jsx,**/*.jsx,*.ts,**/*.ts,*.tsx,**/*.tsx}' && npx eslint '{*.js,**/*.js,*.jsx,**/*.jsx,*.ts,**/*.ts,*.tsx,**/*.tsx,**/*.snap}' --fix --ignore-pattern '/native-base-theme/*'",
    "upgrade": "npx ncu -x 'react, react-dom, @types/react, react-navigation, jest-expo, react-native, react-native-gesture-handler, react-native-reanimated, react-native-appearance, react-navigation-tabs, react-navigation-drawer, react-navigation-stack, @types/react-native, ts-jest'",
    "upgrade:write": "npm run upgrade -- -u"
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.0",
    "axios": "^0.19.0",
    "ethers": "^4.0.40",
    "expo": "^37.0.0",
    "expo-font": "~8.1.0",
    "expo-random": "~8.1.0",
    "expo-secure-store": "~8.1.0",
    "intl": "^1.2.5",
    "native-base": "^2.13.8",
    "react": "16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-37.0.1.tar.gz",
    "react-native-appearance": "~0.3.3",
    "react-native-button": "^2.4.0",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-progress": "^4.0.2",
    "react-native-reanimated": "~1.7.0",
    "react-native-responsive-dimensions": "^3.0.0",
    "react-navigation": "^4.0.10",
    "react-navigation-drawer": "^2.3.3",
    "react-navigation-stack": "^1.10.3",
    "react-navigation-tabs": "^2.5.6",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "tasit-sdk": "0.0.18"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/react": "^16.9.11",
    "@types/react-native": "^0.60.22",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-expo": "^8.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-native": "^3.8.1",
    "expo-cli": "^3.21.10",
    "jest": "^24.9.0",
    "jest-expo": "^37.0.0",
    "jest-prop-type-error": "^1.1.0",
    "npm-check-updates": "^7.0.1",
    "prettier": "^1.19.1",
    "react-dom": "16.9.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.8.3",
    "yargs": "^15.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tasitlabs/tasit.git"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "jest-prop-type-error"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|.*react-navigation|native-base.*|.*expo.*)"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest"
    }
  },
  "keywords": [
    "dapp",
    "ethereum",
    "react",
    "native"
  ],
  "author": "Tasit Labs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tasitlabs/tasit/issues"
  },
  "homepage": "https://tasit.io"
}
