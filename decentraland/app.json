    "slug": "tasit-decentraland",
    "privacy": "unlisted",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.0.24",
    "orientation": "portrait",
    "icon": "./assets/images/TasitLogoFromSvgNoAlpha1024x1024.png",
