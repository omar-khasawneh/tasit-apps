    "android": {
      "package": "io.tasit.decentraland",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/TasitLogoFromSvgTransparent1024x1024.png"
      },
      "icon": "./assets/images/TasitLogoFromSvgTransparentAndOpaque1024x1024.png",
      "permissions": [],
      "versionCode": 24
